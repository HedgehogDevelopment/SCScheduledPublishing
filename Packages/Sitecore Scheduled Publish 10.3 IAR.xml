<project>
  <Metadata>
    <metadata>
      <PackageName>Sitecore Schedule Publish</PackageName>
      <Author>Nehemiah Jeyakumar</Author>
      <Version>10.3 IAR</Version>
      <Revision />
      <License />
      <Comment>This version uses Items as resource files instead of adding Sitecore items</Comment>
      <Attributes />
      <Readme />
      <Publisher>https://github.com/nehemiahj/SCScheduledPublishing</Publisher>
      <PostStep />
      <PackageID />
    </metadata>
  </Metadata>
  <SaveProject>True</SaveProject>
  <Sources>
    <xfiles>
      <Entries>
        <x-item>/App_Config/Include/ZZ_ScheduledPublish/ZZ_ScheduledPublishControl.config</x-item>
        <x-item>/bin/ScheduledPublish.dll</x-item>
        <x-item>/sitecore/shell/Applications/Content Manager/Dialogs/Edit Scheduled Publish/Edit Scheduled Publish.xml</x-item>
        <x-item>/sitecore/shell/Applications/Content Manager/Dialogs/Schedule Publish/Schedule Publish.xml</x-item>
      </Entries>
      <Converter>
        <FileToEntryConverter>
          <Root>/</Root>
          <Transforms>
            <InstallerConfigurationTransform>
              <Options>
                <BehaviourOptions>
                  <ItemMode>Undefined</ItemMode>
                  <ItemMergeMode>Undefined</ItemMergeMode>
                </BehaviourOptions>
              </Options>
            </InstallerConfigurationTransform>
          </Transforms>
        </FileToEntryConverter>
      </Converter>
      <Include />
      <Exclude />
      <Name>File System Items</Name>
    </xfiles>
    <xfiles>
      <Entries>
        <x-item>/App_Data/items/core/items.core.schedule.publish.dat</x-item>
        <x-item>/App_Data/items/master/items.master.schedule.publish.dat</x-item>
      </Entries>
      <Converter>
        <FileToEntryConverter>
          <Root>/</Root>
          <Transforms>
            <InstallerConfigurationTransform>
              <Options>
                <BehaviourOptions>
                  <ItemMode>Undefined</ItemMode>
                  <ItemMergeMode>Undefined</ItemMergeMode>
                </BehaviourOptions>
              </Options>
            </InstallerConfigurationTransform>
          </Transforms>
        </FileToEntryConverter>
      </Converter>
      <Include />
      <Exclude />
      <Name>IAR</Name>
    </xfiles>
  </Sources>
  <Converter>
    <TrivialConverter>
      <Transforms />
    </TrivialConverter>
  </Converter>
  <Include />
  <Exclude />
  <Name />
</project>
